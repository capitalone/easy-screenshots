    package="com.capitalone.easyscreenshots.sample">

    <application
        android:name="com.capitalone.easyscreenshots.sample.QuizApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:screenOrientation="portrait"
        android:theme="@style/AppTheme">
        <activity android:name="com.capitalone.easyscreenshots.sample.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.capitalone.easyscreenshots.sample.SecondActivity" />
        <activity android:name="com.capitalone.easyscreenshots.sample.QuizActivity" />
        <activity android:name="com.capitalone.easyscreenshots.sample.FinalScoreActivity" />
    </application>

</manifest>